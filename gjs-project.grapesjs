"classes":["lightbox-prev"],"attributes":{"type":"button","aria-label":"Precedente","id":"lbPrev"},"components":[{"type":"contentEditable","components":[{"type":"image","name":"Icon Prev","resizable":{"ratioDefault":1},"attributes":{"alt":"Precedente","width":"18","height":"18","src":"https://api.iconify.design/lucide-chevron-left.svg?color=%23112233"}}]}]},{"type":"button","name":"Lightbox Next","classes":["lightbox-next"],"attributes":{"type":"button","aria-label":"Successivo","id":"lbNext"},"components":[{"type":"contentEditable","components":[{"type":"image","name":"Icon Next","resizable":{"ratioDefault":1},"attributes":{"alt":"Successivo","width":"18","height":"18","src":"https://api.iconify.design/lucide-chevron-right.svg?color=%23112233"}}]}]}]}]}]}]},{"type":"script","name":"Interactive Script","components":[{"type":"textnode","content":"\n    const scroller = document.getElementById('galleryScroller');\n    const leftBtn = document.getElementById('scrollLeftBtn');\n    const rightBtn = document.getElementById('scrollRightBtn');\n    const lightbox = document.getElementById('lightbox');\n    const lightboxImg = document.getElementById('lightboxImg');\n    const closeLightbox = document.getElementById('closeLightbox');\n    const lbPrev = document.getElementById('lbPrev');\n    const lbNext = document.getElementById('lbNext');\n    const items = Array.from(document.querySelectorAll('[data-gjs-name=\"Artwork Button\"]'));\n    let currentIndex = -1;\n\n    function scrollByAmount(dir = 1) {\n      const amount = Math.max(280, Math.floor(scroller.clientWidth * 0.8));\n      scroller.scrollBy({ left: dir * amount, behavior: 'smooth' });\n    }\n\n    leftBtn.addEventListener('click', () => scrollByAmount(-1));\n    rightBtn.addEventListener('click', () => scrollByAmount(1));\n\n    function openLightbox(index) {\n      currentIndex = index;\n      const fullSrc = items[currentIndex].getAttribute('data-full');\n      lightboxImg.src = fullSrc;\n      lightbox.classList.remove('hidden');\n      lightbox.classList.add('flex');\n      document.body.style.overflow = 'hidden';\n    }\n    function closeLb() {\n      lightbox.classList.add('hidden');\n      lightbox.classList.remove('flex');\n      lightboxImg.src = '';\n      document.body.style.overflow = '';\n    }\n    function nextItem(step = 1) {\n      if (currentIndex < 0) return;\n      currentIndex = (currentIndex + step + items.length) % items.length;\n      lightboxImg.src = items[currentIndex].getAttribute('data-full');\n    }\n\n    items.forEach((btn, idx) => {\n      btn.addEventListener('click', () => openLightbox(idx));\n      btn.addEventListener('keydown', (e) => {\n        if (e.key === 'Enter' || e.key === ' ') {\n          e.preventDefault();\n          openLightbox(idx);\n        }\n      });\n    });\n\n    closeLightbox.addEventListener('click', closeLb);\n    lightbox.addEventListener('click', (e) => {\n      if (e.target === lightbox) closeLb();\n    });\n    document.addEventListener('keydown', (e) => {\n      if (!lightbox.classList.contains('hidden')) {\n        if (e.key === 'Escape') closeLb();\n        if (e.key === 'ArrowRight') nextItem(1);\n        if (e.key === 'ArrowLeft') nextItem(-1);\n      }\n    });\n    lbPrev.addEventListener('click', () => nextItem(-1));\n    lbNext.addEventListener('click', () => nextItem(1));\n\n    const yearSpan = document.getElementById('yearSpan');\n    yearSpan.textContent = new Date().getFullYear();\n  "}]}],"doctype":"<!DOCTYPE html>","head":{"type":"head","components":[{"tagName":"meta","void":true,"attributes":{"charset":"utf-8"}},{"tagName":"meta","void":true,"attributes":{"name":"viewport","content":"width=device-width,initial-scale=1"}},{"tagName":"title","type":"text","components":[{"type":"textnode","content":"Portfolio Artistico - Home"}]},{"tagName":"link","void":true,"attributes":{"href":"https://fonts.googleapis.com/css2?family=Inter:wght@300;400;500;600&family=Playfair+Display:ital,wght@0,500;0,600;0,700;1,500;1,600&display=swap","rel":"stylesheet"}}]},"docEl":{"tagName":"html","attributes":{"lang":"it"}}},"id":"34oUxyNowRIpXtfD","minHeight":"500px"}],"id":"TZhG76oD1ruyWK1O"}],"symbols":[],"custom":{"projectType":"web","id":""}}